{
	"name": "lang-4d-support",
	"displayName": "lang-4d-support",
	"description": "VS Code extension for supporting 4D language",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.1"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:4d"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"electron-rebuild": "^1.8.5",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"vscode-textmate": "^4.1.1"
	}
}
